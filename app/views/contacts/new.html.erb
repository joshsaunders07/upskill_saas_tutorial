<div class="container">
    <h1 class="text-center">Contact Us</h1>
    
    <%= form_for "/contacts" do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    
    <%= f.label :email %>
    <%= f.text_field :email %>
    
    <%= f.label :message %>
    <%= f.text_field :message %>
    
    <%= f.submit 'Submit' %>
  <% end %>  
    
</div>